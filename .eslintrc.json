{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "standard",
  "parser": "@typecript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-useless-constructor": "off"
  }
}